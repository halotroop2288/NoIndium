{
  "schemaVersion": 1,
  "id": "noindium",
  "version": "${version}",
  "name": "No Indium?",
  "description": "A small, jij-able mod attempting to minimize the usage of Sodium without Indium on Fabric.",
  "authors": [
    "Luligabi",
    "halotroop2288"
  ],
  "contributors": [
    "Felix14-v2",
    "pkstDev"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/no-indium",
    "repo": "https://github.com/Luligabi1/NoIndium",
    "issues": "https://github.com/Luligabi1/NoIndium/issues"
  },
  "license": "MIT",
  "icon": "assets/noindium/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.luligabi.noindium.NoIndium"
    ]
  },
  "mixins": [
    "noindium.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric": "*",
    "minecraft": ">=1.18.2"
  },
